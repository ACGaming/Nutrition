{
	"name": "high_weakness", // Unique Effect ID
	"potion": "weakness",    // Potion's resource location, as used in commands
	"amplifier": 1,          // Potion strength
	"minimum": 0,            // Minimum nutrition level to take effect
	"maximum": 10,           // Maximum nutrition level to take effect
	"detect": "average",     // Accepted values:
	                         //   'any': Any nutrient may be in the threshold
	                         //   'average': The average of all nutrients must be in the threshold
	                         //   'all': All nutrients must be in the threshold
	                         //   'nutrient': A specific nutrient must be in the threshold (see below)
	"nutrient": "",          // If detect=nutrient, this defines the nutrient ID checked against
	"enabled": true          // Will this effect be active or not
}
